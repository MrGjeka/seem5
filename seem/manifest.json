{
  "manifest_version": 3,
  "name": "SeamlessFocus",
  "version": "1.0",
  "permissions": ["identity", "storage", "tabs", "alarms"],
  "host_permissions": ["https://api.spotify.com/*", "https://accounts.spotify.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}